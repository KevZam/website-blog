<%- include("partials/header") -%>
<div class="content">
  <h1>Home</h1>
  <p><%=homeStartingContent%></p>
  <% posts.forEach(function(post) {%>
  <h1><%= post.title %></h1>
  <p><%=post.entry.substring(0, 100) + " ..." %></p>
  <a href="posts/<%=post.title%>">Read More</a>
  <%}) %>
</div>

<%- include("partials/footer") -%>
